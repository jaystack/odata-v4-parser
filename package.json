{
  "name": "@modusign/odata-v4-parser",
  "version": "0.1.0",
  "description": "OData v4 Parser",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "license": "MIT",
  "repository": "https://github.com/modusign/odata-v4-parser",
  "bugs": "https://github.com/modusign/odata-v4-parser/issues",
  "keywords": [
    "OData",
    "v4",
    "parser"
  ],
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rimraf dist && tsc --project tsconfig.build.json",
    "check-type": "tsc --noEmit",
    "format": "prettier --write \"{src,test}/**/*.ts\"",
    "format:check": "prettier --check \"{src,test}/**/*.ts\"",
    "lint": "eslint \"{src,test}/**/*.ts\"",
    "test": "jest",
    "tsc": "tsc -p tsconfig.json --incremental false --noEmit",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/commit-analyzer": "^8.0.1",
    "@semantic-release/git": "^9.0.0",
    "@semantic-release/github": "^7.2.3",
    "@semantic-release/npm": "^7.1.3",
    "@semantic-release/release-notes-generator": "^9.0.3",
    "@types/jest": "^24.9.1",
    "@types/node": "^16.4.12",
    "@typescript-eslint/eslint-plugin": "^4.28.5",
    "@typescript-eslint/parser": "^4.28.5",
    "chai": "^4.1.2",
    "concurrently": "^3.5.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.31.0",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-import": "^2.19.1",
    "husky": "^7.0.4",
    "istanbul": "^0.4.5",
    "jest": "^27.0.6",
    "lint-staged": "^11.1.1",
    "mocha": "^5.2.0",
    "mochawesome": "^3.0.2",
    "prettier": "^2.3.2",
    "remap-istanbul": "^0.11.1",
    "rimraf": "^3.0.2",
    "semantic-release": "^17.4.4",
    "semantic-release-slack-bot": "^2.1.1",
    "ts-jest": "^27.0.5",
    "tslint": "^5.10.0",
    "typedoc": "^0.11.1",
    "typedoc-default-themes": "^0.5.0",
    "typedoc-plugin-external-module-name": "^1.1.1",
    "typescript": "^3.9.10"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "./coverage",
    "testEnvironment": "node"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
